public class JavaApplication11 {

    public static void main(String[] args) {
        
        Lobo x = new Lobo();
        Cachorro y = new Cachorro();
        
        x.emitirSom(); 
        y.emitirSom();
        
        y.reagir("Ol√°");
        y.reagir("Toma Comida");
